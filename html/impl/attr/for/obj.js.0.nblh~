/**********************************************************************
 *
 * for_/obj.js
 *
 * author: Patrick Dooley
 *
 *
 **********************************************************************/

define
(
	[
		"../base/obj"
	],
	function
	(
		base
	)
	{

	//*******************************************************************//
	//***																	   
	//***		public initializors
	//***
	//*******************************************************************//
	
		function for_() 
		{
			//console.log("for_::constructor");
			this.m_sTag	= "for"
		}

		for_.prototype = new base();
		for_.prototype.constructor = for_;
		for_.constructor = base.prototype.constructor;

		for_.prototype.check = function check() 
		{
			base.prototype.check.call(this);
			//console.log(this.logCalled());
		};

		for_.prototype.init = function init() 
		{
			//console.log(this.logCalled());
			base.prototype.init.call(this);
		};

	//*******************************************************************//
	//***																	   
	//***		public accessors
	//***
	//*******************************************************************//

	//*******************************************************************//
	//***																	   
	//***		public callbacks
	//***
	//*******************************************************************//

	//*******************************************************************//
	//***																	   
	//***		public methods
	//***
	//*******************************************************************//

	//*******************************************************************//
	//***																	   
	//***		private methods (non-overrides)
	//***
	//*******************************************************************//

		for_.prototype.baseInitOR = function()
		{
			this.for_Init(); 
		};
		
	//*******************************************************************//
	//***																	   
	//***		private methods (overrides)
	//***
	//*******************************************************************//

	//*******************************************************************//
	//***																	   
	//***		private methods
	//***
	//*******************************************************************//
		
		for_.prototype.for_Init = 
			function for_Init()
		{
			//console.log(this.logCalled()); 
		};
		
		return for_;

	}
);
